{
	"manifest_version": 2,
	"name": "__MSG_name__",
	"version": "1.10",
	"applications": {
		"gecko": {
			"id": "@mute-sites-in-blacklist"
		}
	},
	"description": "__MSG_description__",
	"homepage_url": "https://github.com/MineBill/mute-sites-in-blacklist",
	"icons": {
		"48": "icons/icon-48.png",
		"96": "icons/icon-96.png"
	},
	"default_locale": "en",
	"permissions": [
		"storage",
		"tabs"
	],
	"options_ui": {
		"page": "options/options.html",
		"browser_style": true
	},
	"background": {
		"scripts": [
			"common.js",
			"background/background.js"
		]
	}
}
